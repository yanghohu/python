git init  -- 저장소 초기화 하기

-- 전역사용자 등록
git config --global user.name "yanghohu"
git config --global user.email yanghohu@gmail.com

-- version 관리할 폴더 지정
git add python

-- 첫번째 커밋
git commit -m "첫번째 git 폴더추가"

-- 로그파일
git log

-- 원격저장소 알려주는 명령어
git remote add origin https://github.com/yanghohu/python

-- 만든버전 push하기
git push origin master

-- 원격저장소에 올린 파일 받아오기
git clone https://github.com/yanghohu/python/tree/main . # .은 현재폴더에 풀어라

-- 변경사항 내 컴퓨터로 가져오기 git pull
git pull origin master

-- Gui로 하기
커밋, push

